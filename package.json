{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "dist/engine.js",
  "types": "dist/engine.d.ts",
  "directories": {
    "test": "tests"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "webpack && node dist/engine.js && cp -r ./src/shaders/. ./dist/shaders/",
    "test": "webpack --config webpack.test.config.js && node ./tests/tier0.test.js",
    "example": "tsc && node ./examples/run.js --",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve --history-api-fallback  "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timofeji/sim-kit.git"
  },
  "keywords": [
    "game",
    "engine",
    "simulation",
    "rendering"
  ],
  "author": "timofej jermolaev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timofeji/sim-kit/issues"
  },
  "homepage": "https://github.com/timofeji/sim-kit#readme",
  "devDependencies": {
    "@types/node": "^17.0.32",
    "@types/webpack-dev-server": "^4.7.2",
    "@webpack-cli/generators": "^2.4.2",
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "querystring": "^0.2.1"
  }
}
